{
    "name": "modules/file-manager-module",
    "description": "Professional file management module for SaaS with S3 support, signed URLs, audit logs, and tenant isolation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ModulPanel",
            "email": "info@modulpanel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "S3 storage için gereklidir (^3.0)",
        "modules/image-upload-module": "Resim işleme özellikleri için (resize, thumbnail)",
        "modules/rbac-module": "Audit logs ve gelişmiş permission sistemi için (opsiyonel)"
    },
    "autoload": {
        "psr-4": {
            "Modules\\FileManagerModule\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\FileManagerModule\\Providers\\FileManagerModuleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
