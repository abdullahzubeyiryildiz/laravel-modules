{
    "name": "modules/image-upload-module",
    "description": "Reusable image upload module for Laravel with S3 and public storage support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdullah Zubeyir Yıldız",
            "email": "abdullahzubeyiryildiz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "S3 storage için gereklidir (^3.0)"
    },
    "autoload": {
        "psr-4": {
            "Modules\\ImageUploadModule\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\ImageUploadModule\\Providers\\ImageUploadModuleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
