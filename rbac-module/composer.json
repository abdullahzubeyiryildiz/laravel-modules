{
    "name": "modules/rbac-module",
    "description": "Role-Based Access Control (RBAC) module for SaaS with roles, permissions, and audit logs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ModulPanel",
            "email": "info@modulpanel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\RbacModule\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\RbacModule\\Providers\\RbacModuleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
